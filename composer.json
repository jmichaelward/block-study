    "type": "wordpress-plugin",
    "authors": [
        {
            "name": "Jeremy Ward",
            "email": "jeremy@jmichaelward.com"
        }
    ],
    "require": {
        "webdevstudios/oops-wp": "dev-master",
        "jmichaelward/hello-gutenberg": "dev-master"
    },
    "repositories": [
        {
            "type": "composer",

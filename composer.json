{
    "name": "jmichaelward/learning-gutenberg",
    "description": "A WordPress plugin for me to learn how to create custom Gutenberg blocks.",
    "type": "wordpress-plugin",
    "authors": [
        {
            "name": "Jeremy Ward",
            "email": "jeremy@jmichaelward.com"
        }
    ],
    "require": {
        "wpackagist-plugin/gutenberg": "dev-master"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "JMichaelWard\\LearningGutenberg\\": "src/"
        }
    }
}
